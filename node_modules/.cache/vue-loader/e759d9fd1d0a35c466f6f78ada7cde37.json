} from 'vuex'\nexport default {\n  name: 'App',\n  components: {\n    SlotList\n  },\n  computed: {\n    // ...mapState(['total'])\n    // ...mapState({\n    //   total: \"total\"\n    // })\n    ...mapState({\n      total: (state)=>state.total\n    })\n  },\n   \n  \n  data() {\n    return {\n      users: [\n        {name:\"张三\",age:19},\n        {name:\"李四\",age:29},\n        {name:\"王五\",age:39},\n      ],\n      num: 0\n    }\n  },\n  methods: {\n    ...mapMutations([\"DEC_COUNT\",\"INC_COUNT\"]),\n    // ...mapActions([\"decTotal\",\"incTotal\"]),\n    ...mapActions({\n      incTotal: \"incTotal\",\n      decTotal: \"decTotal\"\n    }),\n    del(index){\n      // console.log(index)\n      this.users.splice(index,1)\n    },\n    handleDec(){\n      // this.$store.commit(\"DEC_COUNT\",{total:--this.num})\n      // this.DEC_COUNT({total:--this.num})\n      // this.$store.dispatch('decTotal',{total:--this.num})\n      this.decTotal({total:--this.num})\n    },\n    handleInc(){\n      // this.$store.commit(\"INC_COUNT\",{total:++this.num})\n      // this.INC_COUNT({total:++this.num})\n      this.incTotal({total:++this.num})\n    }\n  },\n  created() {\n    console.log(process.env.VUE_APP_DOME)\n  },\n}\n</script>\n\n<style>\n\n</style>\n"]}]}
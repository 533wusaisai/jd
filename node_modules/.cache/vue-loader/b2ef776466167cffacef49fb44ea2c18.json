n  data() {\n    return {\n      users: [\n        {name:\"张三\",age:19},\n        {name:\"李四\",age:29},\n        {name:\"王五\",age:39},\n      ]\n    }\n  },\n  methods: {\n    del(index){\n      // console.log(index)\n      this.users.splice(index,1)\n    }\n  },\n  created() {\n    console.log(process.env.VUE_APP_DOME)\n  },\n}\n</script>\n\n<style>\n\n</style>\n"]}]}
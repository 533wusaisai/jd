otal:--this.num})\n    },\n    handleInc(){\n      // this.$store.commit(\"INC_COUNT\",{total:++this.num})\n      // this.INC_COUNT({total:++this.num})\n      this.incTotal({total:++this.num})\n    }\n  },\n  created() {\n    console.log(process.env.VUE_APP_DOME)\n  },\n}\n\n</script>"],"sourceRoot":"src/pages/home"}]}